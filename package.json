{
  "name": "internetfriendsforever",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "node build",
    "dev": "concurrently \"npm run serve\" \"npm run watch\"",
    "watch": "chokidar --initial \"src/**/*.js\" -c \"npm run build\"",
    "serve": "serve -l 3000 dist",
    "clean": "rm -rf dist",
    "test": "eslint src/**/*.js"
  },
  "dependencies": {
    "@cyberspace/styles": "^0.3.20",
    "@sanity/block-content-to-html": "^1.3.8",
    "@sanity/client": "^1.149.7",
    "@sanity/image-url": "^0.140.18",
    "pretty": "^2.0.0"
  },
  "devDependencies": {
    "chokidar-cli": "^2.1.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "serve": "^11.3.2"
  }
}
